'react'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { Link, useParams, useNavigate } from 'react-router-dom'\nimport { Row, Col, Image, ListGroup, Button, Card, Form } from 'react-bootstrap'\nimport Rating from '../components/Rating'\nimport { listProductDetails } from '../actions/productActions'\nimport Loader from '../components/Loader'\nimport Message from '../components/Message'\n// import axios from 'axios'\n\n\nfunction ProductScreen({ history }) {\n    const [qty, setQty] = useState(1)\n\n    const { id } = useParams();    \n    const dispatch = useDispatch()\n    const productDetails = useSelector(state => state.productDetails)\n    const { loading, error, product } = productDetails\n    const navigate = useNavigate()\n\n    useEffect(() => {\n        dispatch(listProductDetails(id))\n    }, [dispatch])\n\n    const addToCardHandler = () => {\n        navigate(`/cart/${id}?qty=${qty}`)\n    }\n\n    return (\n    <div>\n      <Link to='/' className='btn btn-light my-3'>Go Back</Link>\n\n        {loading ?\n            <Loader />\n            : error\n            ? <Message variant='danger'>{error}</Message>\n            :(\n                <Row>\n        <Col md={6}>\n            <Image src={product.image} alt={product.name} fluid />\n        </Col>\n        <Col md={3}>\n            <ListGroup variant='flush'>\n                <ListGroup.Item>\n                    <h3>{product.name}</h3>\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                    <Rating value={product.rating} text={`${product.numReviews} reviews`} color={'#f8e825'} />\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                    Price: ${product.price}\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                    Description: {product.description}\n                </ListGroup.Item>\n\n            </ListGroup>\n        </Col>\n        <Col md={3}>\n            <Card>\n                <ListGroup variant='flush'>\n                    <ListGroup.Item>\n                        <Row>\n                            <Col>Price:</Col>\n                            <Col>\n                                <strong>${product.price}</strong>\n                            </Col>\n                        </Row>\n                    </ListGroup.Item>\n\n                    <ListGroup.Item>\n                        <Row>\n                            <Col>Status:</Col>\n                            <Col>\n                                {product.countInStock > 0 ? 'In Stock' : 'Out of Stock'} \n                            </Col>\n                        </Row>\n                    </ListGroup.Item>\n\n                    {product.countInStock > 0 && (\n                        <ListGroup.Item>\n                            <Row>\n                                <Col>Qty</Col>\n                                <Col xs='auto' className='my-1'>\n                                    <Form.Control as=\"select\" value={qty} onChange={(e) => setQty(e.target.value)}>\n                                        {\n                                            [...Array(product.countInStock).keys()].map((x) => (\n                                                <option key={x + 1} value={x + 1}>\n                                                    {x + 1}\n                                                </option>\n                                            ))\n                                        }\n                                    </Form.Control>\n                                </Col>\n                            </Row>\n                        </ListGroup.Item>\n                    )}\n\n                    <ListGroup.Item>\n                        <Button onClick={addToCardHandler} className='w-100' disabled={product.countInStock == 0} type='button'  >Add to Cart</Button>\n                    </ListGroup.Item>\n\n                </ListGroup>\n            </Card>\n        </Col>\n      </Row>\n            )\n        }\n\n      \n    </div>\n  )\n}\n\nexport default ProductScreen",[],"/home/joke/Videos/ecommerce/frontend/src/screens/HomeScreen.js",["164","165"],[],"/home/joke/Videos/ecommerce/frontend/src/components/Footer.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Header.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/products.js",[],[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Rating.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Product.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/store.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/reducer/productReducers.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/constants/productConstants.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/actions/productActions.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Loader.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Message.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/screens/CartScreen.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/reducer/cartReducers.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/constants/cartConstants.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/actions/cartAction.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/reducer/userReducers.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/constants/userConstants.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/screens/LoginScreen.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/actions/userActions.js",["166"],[],"/home/joke/Videos/ecommerce/frontend/src/components/FormContainer.js",["167"],[],"/home/joke/Videos/ecommerce/frontend/src/screens/RegisterScreen.js",["168"],[],"/home/joke/Videos/ecommerce/frontend/src/screens/ProfileScreen.js",["169"],[],"/home/joke/Videos/ecommerce/frontend/src/screens/ShippingScreen.js",["170","171"],[],"/home/joke/Videos/ecommerce/frontend/src/components/CheckoutSteps.js",[],[],"/home/joke/Videos/ecommerce/frontend/sr