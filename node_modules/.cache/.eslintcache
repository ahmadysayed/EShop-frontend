 <ListGroup.Item>\n                            <Row>\n                                <Col>Qty</Col>\n                                <Col xs='auto' className='my-1'>\n                                    <Form.Control as=\"select\" value={qty} onChange={(e) => setQty(e.target.value)}>\n                                        {\n                                            [...Array(product.countInStock).keys()].map((x) => (\n                                                <option key={x + 1} value={x + 1}>\n                                                    {x + 1}\n                                                </option>\n                                            ))\n                                        }\n                                    </Form.Control>\n                                </Col>\n                            </Row>\n                        </ListGroup.Item>\n                    )}\n\n                    <ListGroup.Item>\n                        <Button onClick={addToCardHandler} className='w-100' disabled={product.countInStock == 0} type='button'  >Add to Cart</Button>\n                    </ListGroup.Item>\n\n                </ListGroup>\n            </Card>\n        </Col>\n      </Row>\n            )\n        }\n\n      \n    </div>\n  )\n}\n\nexport default ProductScreen",[],"/home/joke/Videos/ecommerce/frontend/src/screens/HomeScreen.js",["120","121"],[],"/home/joke/Videos/ecommerce/frontend/src/components/Footer.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Header.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/products.js",[],[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Rating.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Product.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/store.js",["122"],[],"/home/joke/Videos/ecommerce/frontend/src/reducer/productReducers.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/constants/productConstants.js",[],[],"/home/joke/Videos/ecommerce/fr