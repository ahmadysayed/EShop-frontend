/ecommerce/frontend/src/App.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/screens/ProductScreen.js",["198","199"],[],"import React, { useState, useEffect } from 'react'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { Link, useParams, useNavigate } from 'react-router-dom'\nimport { Row, Col, Image, ListGroup, Button, Card, Form } from 'react-bootstrap'\nimport Rating from '../components/Rating'\nimport { listProductDetails } from '../actions/productActions'\nimport Loader from '../components/Loader'\nimport Message from '../components/Message'\n// import axios from 'axios'\n\n\nfunction ProductScreen() {\n    const [qty, setQty] = useState(1)\n\n    const { id } = useParams();    \n    const dispatch = useDispatch()\n    const productDetails = useSelector(state => state.productDetails)\n    const { loading, error, product } = productDetails\n    const navigate = useNavigate()\n\n    useEffect(() => {\n        dispatch(listProductDetails(id))\n    }, [dispatch])\n\n    const addToCardHandler = () => {\n        navigate(`/cart/${id}?qty=${qty}`)\n    }\n\n    return (\n    <div>\n      <Link to='/' className='btn btn-light my-3'>Go Back</Link>\n\n        {loading ?\n            <Loader />\n            : error\n            ? <Message variant='danger'>{error}</Message>\n            :(\n                <Row>\n        <Col md={6}>\n            <Image src={product.image} alt={product.name} fluid />\n        </Col>\n        <Col md={3}>\n            <ListGroup variant='flush'>\n                <ListGroup.Item>\n                    <h3>{product.name}</h3>\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                    <Rating value={product.rating} text={`${product.numReviews} reviews`} color={'#f8e825'} />\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                    Price: ${product.price}\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                    Description: {product.description}\n                </ListGroup.Item>\n\n            </ListGroup>\n        </Col>\n        <Col md={3}>\n            <Card>\n                <ListGroup variant='flush'>\n                    <ListGroup.Item>\n                        <Row>\n                            <Col>Price:</Col>\n                            <Col>\n                                <strong>${product.price}</strong>\n                            </Col>\n                        </Row>\n                    </ListGroup.Item>\n\n                    <ListGroup.Item>\n                        <Row>\n                            <Col>Status:</Col>\n                            <Col>\n                                {product.countInStock > 0 ? 'In Stock' : 'Out of Stock'} \n                            </Col>\n                        </Row>\n                    </ListGroup.Item>\n\n                    {product.countInStock > 0 && (\n                        <ListGroup.Item>\n                            <Row>\n                                <Col>Qty</Col>\n                                <Col xs='auto' className='my-1'>\n                                    <Form.Control as=\"select\" value={qty} onChange={(e) => setQty(e.target.value)}>\n                                        {\n                                            [...Array(product.countInStock).keys()].map((x) => (\n                                                <option key={x + 1} value={x + 1}>\n                                                    {x + 1}\n                                                </option>\n                                            ))\n                                        }\n                                    </Form.Control>\n                                </Col>\n                            </Row>\n                        </ListGroup.Item>\n                    )}\n\n                    <ListGroup.Item>\n                        <Button onClick={addToCardHandler} className='w-100' disabled={product.countInStock == 0} type='button'  >Add to Cart</Button>\n                    </ListGroup.Item>\n\n                </ListGroup>\n            </Card>\n        </Col>\n      </Row>\n            )\n        }\n\n      \n    </div>\n  )\n}\n\nexport default ProductScreen",[],"/home/joke/Videos/ecommerce/frontend/src/screens/HomeScreen.js",["200","201"],[],"/home/joke/Videos/ecommerce/frontend/src/components/Footer.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Header.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/products.js",[],[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Rating.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Product.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/store.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/reducer/productReducers.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/constants/productConstants.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/actions/productActions.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Loader.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/components/Message.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/screens/CartScreen.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/reducer/cartReducers.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/constants/cartConstants.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/actions/cartAction.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/reducer/userReducers.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/constants/userConstants.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/screens/LoginScreen.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/actions/userActions.js",["202"],[],"/home/joke/Videos/ecommerce/frontend/src/components/FormContainer.js",["203"],[],"/home/joke/Videos/ecommerce/frontend/src/screens/RegisterScreen.js",["204"],[],"/home/joke/Videos/ecommerce/frontend/src/screens/ProfileScreen.js",["205"],[],"/home/joke/Videos/ecommerce/frontend/src/screens/ShippingScreen.js",["206","207"],[],"/home/joke/Videos/ecommerce/frontend/src/components/CheckoutSteps.js",[],[],"/home/joke/Videos/ecommerce/frontend/src/screens/PaymentScreen.js",["208","209"],[],"/home/joke/Videos/ecommerce/frontend/src/screens/PlaceOrderScreen.js",["210","211","212"],[],"import React, { useState, useEffect } from 'react'\nimport { useLocation, useNavigate } from 'react-router-dom'\nimport { Button, Row, Col, ListGroup, Image, Card } from 'react-bootstrap'\nimport { Link } from 'react-router-dom'\nimport { useDispatch, useSelector } from 'react-redux'\nimport Message from '../components/Message'\nimport CheckoutSteps from '../components/CheckoutSteps'\nimport { createOrder } from '../actions/orderAction'\nimport { ORDER_CREATE_RESET } from '../constants/orderConstants'\n\nfunction PlaceOrderScreen() {\n\n    const history = useNavigate()\n\n    const orderCreate = useSelector(state =>  state.orderCreate)\n    const { order, error, success } = orderCreate\n\n    const dispatch = useDispatch()\n\n\n    const cart = useSelector(state => state.cart)\n\n    cart.itemsPrice = cart.cartItems.reduce((acc, item) => acc + item.price * item.qty, 0).toFixed(2)\n    cart.shippingPrice = (cart.itemsPrice > 100 ? 0 : 10).toFixed(2)\n    cart.taxPrice = Number((0.082) * cart.itemsPrice).toFixed(2)\n\n    cart.totalPrice = (Number(cart.itemsPrice) + Number(cart.shippingPrice) + Number(cart.taxPrice)).toFixed(2) \n\n    if (!cart.paymentMethod) {\n        history('/payment ')\n    }\n    \n    useEffect(() => {\n        if(success) {\n            history(`/order/${order._id}`)\n            dispatch({ type: ORDER_CREATE_RESET })\n        }\n    }, [success, history])\n\n    const placeOrder = () => {\n        dispatch(createOrder({\n            orderItems: cart.cartItems,\n            shippingAddress: cart.shippingAddress,\n            paymentMethod: cart.paymentMethod,\n            itemsPrice: cart.itemsPrice,\n            shippingPrice: cart.shippingPrice,\n            taxPrice: cart.taxPrice,\n            totalPrice: cart.totalPrice,\n        }))\n    }\n\n  return (\n    <div>\n        <CheckoutSteps step1 step2 step3 step4 />\n        <Row>\n            <Col md={8}>\n                <ListGroup variant='flush'>\n                    <ListGroup.Item>\n                        <h2>Shipping</h2>\n\n                        <p>\n                            <strong>Shipping: </strong>\n                            {cart.shippingAddress.address}, {cart.shippingAddress.city}\n                            {'  '}\n                            {cart.shippingAddress.postalCode},\n                            {'  '}\n                            {cart.shippingAddress.country} \n                        </p>\n                    </ListGroup.Item>\n\n                    <ListGroup.Item>\n                        <h2>Payment Method</h2>\n\n                        <p>\n                            <strong>Method: </strong>\n                            {cart.paymentMethod} \n                        </p>\n                    </ListGroup.Item>\n\n\n                    <ListGroup.Item>\n                        <h2>Order Items</h2>\n\n                        {cart.cartItems.length === 0 ? <Message variant='info'> \n                            Your cart is empty\n                        </Message> : (\n                            <ListGroup variant='flush'>\n                                {cart.cartItems.map((item, index) => (\n                                    <ListGroup.Item key={index}>\n                                        <Row>\n                                            <Col md={1}>\n                                                <Image src={item.image} alt={item.name} fluid rounded />\n                                            </Col>\n\n                                            <Col>\n                                                <Link to={`/product/${item.product}`}>{item.name}</Link>\n                                            